<svg width="400" height="500" viewBox="0 0 400 500" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="400" height="500" fill="#FFF7ED"/>
  
  <!-- Chef hat -->
  <ellipse cx="200" cy="80" rx="60" ry="30" fill="#FFFFFF" stroke="#E5E7EB" stroke-width="2"/>
  <rect x="140" y="80" width="120" height="40" fill="#FFFFFF" stroke="#E5E7EB" stroke-width="2"/>
  
  <!-- Chef hat pleats -->
  <path d="M 160 85 Q 165 70 170 85" stroke="#E5E7EB" stroke-width="1" fill="none"/>
  <path d="M 180 85 Q 185 70 190 85" stroke="#E5E7EB" stroke-width="1" fill="none"/>
  <path d="M 200 85 Q 205 70 210 85" stroke="#E5E7EB" stroke-width="1" fill="none"/>
  <path d="M 220 85 Q 225 70 230 85" stroke="#E5E7EB" stroke-width="1" fill="none"/>
  <path d="M 240 85 Q 235 70 230 85" stroke="#E5E7EB" stroke-width="1" fill="none"/>
  
  <!-- Face -->
  <ellipse cx="200" cy="180" rx="50" ry="60" fill="#D2691E"/>
  
  <!-- Eyes -->
  <ellipse cx="185" cy="165" rx="8" ry="6" fill="#FFFFFF"/>
  <ellipse cx="215" cy="165" rx="8" ry="6" fill="#FFFFFF"/>
  <circle cx="185" cy="165" r="4" fill="#000000"/>
  <circle cx="215" cy="165" r="4" fill="#000000"/>
  <circle cx="186" cy="163" r="1.5" fill="#FFFFFF"/>
  <circle cx="216" cy="163" r="1.5" fill="#FFFFFF"/>
  
  <!-- Eyebrows -->
  <path d="M 175 155 Q 185 150 195 155" stroke="#000000" stroke-width="2" fill="none"/>
  <path d="M 205 155 Q 215 150 225 155" stroke="#000000" stroke-width="2" fill="none"/>
  
  <!-- Nose -->
  <ellipse cx="200" cy="180" rx="4" ry="8" fill="#CD853F"/>
  
  <!-- Mouth -->
  <path d="M 185 200 Q 200 210 215 200" stroke="#000000" stroke-width="2" fill="none"/>
  
  <!-- Chef uniform -->
  <rect x="120" y="240" width="160" height="200" fill="#FFFFFF" stroke="#E5E7EB" stroke-width="2"/>
  
  <!-- Chef uniform buttons -->
  <circle cx="200" cy="280" r="4" fill="#E5E7EB"/>
  <circle cx="200" cy="310" r="4" fill="#E5E7EB"/>
  <circle cx="200" cy="340" r="4" fill="#E5E7EB"/>
  <circle cx="200" cy="370" r="4" fill="#E5E7EB"/>
  
  <!-- Arms -->
  <ellipse cx="100" cy="320" rx="20" ry="60" fill="#FFFFFF" stroke="#E5E7EB" stroke-width="2"/>
  <ellipse cx="300" cy="320" rx="20" ry="60" fill="#FFFFFF" stroke="#E5E7EB" stroke-width="2"/>
  
  <!-- Hands -->
  <circle cx="100" cy="380" r="15" fill="#D2691E"/>
  <circle cx="300" cy="380" r="15" fill="#D2691E"/>
  
  <!-- Cooking utensils -->
  <!-- Wooden spoon in left hand -->
  <rect x="95" y="370" width="3" height="25" fill="#8B4513"/>
  <ellipse cx="96.5" cy="368" rx="6" ry="3" fill="#8B4513"/>
  
  <!-- Whisk in right hand -->
  <rect x="298" y="370" width="2" height="20" fill="#A8A8A8"/>
  <circle cx="299" cy="368" r="4" fill="none" stroke="#A8A8A8" stroke-width="1"/>
  <path d="M 295 368 L 303 368 M 297 366 L 301 370 M 297 370 L 301 366" stroke="#A8A8A8" stroke-width="1"/>
  
  <!-- Apron -->
  <path d="M 140 260 L 140 420 L 260 420 L 260 260" fill="#E8630A" opacity="0.8"/>
  <rect x="180" y="240" width="40" height="20" fill="#E8630A" opacity="0.8"/>
  
  <!-- Apron strings -->
  <path d="M 140 260 Q 120 250 110 270" stroke="#E8630A" stroke-width="2" fill="none"/>
  <path d="M 260 260 Q 280 250 290 270" stroke="#E8630A" stroke-width="2" fill="none"/>
  
  <!-- KOKO DELICACY logo on apron -->
  <text x="200" y="350" font-family="Arial, sans-serif" font-size="16" font-weight="bold" text-anchor="middle" fill="#FFFFFF">KOKO</text>
  <text x="200" y="370" font-family="serif" font-size="12" font-style="italic" text-anchor="middle" fill="#FFFFFF">DELICACY</text>
  
  <!-- Professional chef smile -->
  <path d="M 185 200 Q 200 215 215 200" stroke="#000000" stroke-width="3" fill="none" stroke-linecap="round"/>
</svg> 